# Frontend Environment Variables
# Copy this file to .env and fill in your values

# Backend API URL (optional - defaults to production if not set)
# For local development: http://localhost:8000
# For production: https://getting-started-with-gemini.onrender.com
REACT_APP_BACKEND_URL=

# Firebase Configuration (REQUIRED for Firebase Auth)
# If these are not set, the app will fallback to simple JWT-based auth
# Get these values from Firebase Console > Project Settings
REACT_APP_FIREBASE_API_KEY=
REACT_APP_FIREBASE_AUTH_DOMAIN=
REACT_APP_FIREBASE_PROJECT_ID=
